// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Devs {
  id String @id @default(cuid())
  firstName String
  lastName String
  email String @unique
  password String
}

model Users {
  id String @id @default(cuid())
  firstName String
  lastName String
  email String @unique
  born  String
  cpf String @unique
  cellphone String @unique 
}